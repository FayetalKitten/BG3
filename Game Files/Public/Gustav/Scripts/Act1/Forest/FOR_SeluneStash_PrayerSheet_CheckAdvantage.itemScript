INIT
	ITEM:__Me

EVENTS

EVENT PUZZLE_HiddenPerception_CheckAdvantage
VARS
	CHARACTER:_Char
ON
	OnFunction("PUZZLE_HiddenPerception_CheckAdvantage")
ACTIONS
	IF "c1&c2"
		GetVar(_Char,__Me,"CheckAdvantage_Character")
		IsTagged(_Char,"SELUNE")
	THEN
		SetVar(__Me,"CheckAdvantage_Result",INT:2)
	ENDIF
