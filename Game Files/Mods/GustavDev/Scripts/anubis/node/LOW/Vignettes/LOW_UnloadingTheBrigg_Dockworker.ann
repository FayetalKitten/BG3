game.states.LOW_UnloadingTheBrigg_Dockworker = State{function()

    modules = {"shared.moveto"}

    local dockWorker_02 = Entity("S_LOW_UnloadingTheBrigg_Dockworker_02_d01f0d0a-3f2f-4c86-8bcb-92a79897bd17")
    local dockWorker_03 = Entity("S_LOW_UnloadingTheBrigg_Dockworker_03_56660521-1b82-40a2-8cba-aec5e34235d2")

    local liftingAnimation_Start = Animation("CUST_TryingToLiftHeavyBox_01_Start_fb6ede7d-0ef5-4f41-b079-1f863a43cc4e")
    local liftingAnimation_Loop = Animation("CUST_TryingToLiftHeavyBox_01_Loop_0ed449b0-aaf9-43cb-9838-04161502aaf5")
    local liftingAnimation_End = Animation("CUST_TryingToLiftHeavyBox_01_End_f866422f-a64c-4428-a699-6ed1eea6bf42")

    local liftingTrigger

    self.OnInit = function()
        if (me == dockWorker_02) then
            liftingTrigger = Entity("S_LOW_UnloadingTheBrigg_Dockworker_02_Trigger_97b7cbda-d558-44ac-be05-6e4ae4fc3b36")
        elseif (me == dockWorker_03) then
            liftingTrigger = Entity("S_LOW_UnloadingTheBrigg_Dockworker_03_Trigger_0fb3b70b-de65-410b-8c82-a97be12aa663")
        end
    end

    nodes.MoveToLiftingPosition = Action{
        function()
            Sleep(math.random(1.0,3.0))
            mod.moveto.MoveToPoint(liftingTrigger, MovementSpeed.Walk, me, 0.1)
        end,

        Valid = function()
            return liftingTrigger ~= nil and GetDistanceTo(me, liftingTrigger) >= 0.2
        end
    }

    nodes.Lifting = Action {        
        function()
            LookFrom(liftingTrigger)
            Sleep(math.random(1.0,5.0))
            PlayLoopingAnimation(liftingAnimation_Start,{liftingAnimation_Loop},liftingAnimation_End,false)
            WaitForInterrupt()
        end,

        OnLeave = function()
            StopAnimation(me)
        end
    }
end}