game.states.LOW_SerialKiller_Orphans = State{function ()

    --------------------------------Modules--------------------------------
    modules = {"shared.dialog", "shared.moveto", "shared.math"}
    --------------------------------Parameters--------------------------------

    params.CharacterAD = {type = EParamType.String, help = "[[Ad for this character]]", required = true }
    params.WanderPoint = {type = EParamType.String, help = "[[WanderPoint for the childs at Highberrys House]]", required = true}
    params.KitchenPoint = {type = EParamType.String, help = "[[Kitchen point to eat something there]]", required = true }
    --------------------------------Local variables--------------------------------

    --------------------------------Nodes--------------------------------

    nodes = Selector {
        function(nodes)
            return FindDifferentRandomSelectable(nodes)
        end
    }

    nodes.Wander = Action {
        function()
            Wander(5.0, 4.0, MovementSpeed.Stroll, Entity(params.WanderPoint))
            Sleep(12 + 18 * math.random())
        end
    }

    nodes.TellingAD = Action {
        function()
            if (mod.math.CoinToss()) then
                mod.dialog.StartCheckedAutomatedDialog(
                    Dialog(params.CharacterAD),
                    {
                        waitForCompletion = true
                    },
                    me
                )
                Sleep(15 + 20 * math.random())
            end
        end,
        CanEnter = function()
            return params.CharacterAD ~= nil
        end,
    }

    nodes.GoKitchenAndEat = Action {
        function()
            Sleep(15 + 20 * math.random())
            PlayAnimation(Animation("CUST_Eating_01_e1bc92e7-6f50-463d-85a5-c66352c001b6"))
        end
    }


end
}